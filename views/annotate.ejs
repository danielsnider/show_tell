    function drop(ev) {
        ev.preventDefault();
        var data = ev.dataTransfer.getData("Text");
        var copyimg = document.createElement("img");
        var original = document.getElementById(data);
        copyimg.src = original.src;
    copyimg.width = 700;
    copyimg.height =400;
        ev.target.appendChild(copyimg);
    }
